{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "import/no-extraneous-dependencies": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-restricted-globals": "off",
    "linebreak-style": "off"
    // "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
